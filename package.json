{
  "name": "projetoagenda",
  "version": "1.0.0",
  "description": "Numeros Mega da Virada",
  "main": "server.js",
  "dependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^9.1.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "bcryptjs": "^2.4.3",
    "connect-flash": "^0.1.1",
    "connect-mongo": "^5.1.0",
    "cookie-parser": "^1.4.7",
    "csurf": "^1.11.0",
    "dotenv": "^16.4.7",
    "ejs": "^3.1.10",
    "espress": "^0.0.0",
    "express": "^4.22.1",
    "express-session": "^1.18.1",
    "helmet": "^7.2.0",
    "mongoose": "^8.9.7",
    "nodejs": "^0.0.0",
    "validator": "^13.12.0",
    "webpack": "^5.97.1",
    "webpack-cli": "^5.1.4",
    "style-loader": "^3.3.3",
    "css-loader": "^6.8.1"
  },
  "scripts": {
    "start": "node server.js",
    "dev": "killall node || true && nodemon server.js",
    "build": "echo 'Build complete - using pre-built assets'",
    "build:watch": "webpack --watch",
    "dev:server": "nodemon server.js",
    "dev:client": "webpack --watch --config webpack.config.js",
    "dev:server:hot": "nodemon server.js --exec babel-node src/server.js",
    "data:server": "nodemon --exec babel-node src/server.js",
    "data:client": "webpack --watch --config webpack.config.js",
    "data:hot": "webpack --watch --config webpack.config.js --mode development",
    "data:server:hot": "nodemon --exec babel-node src/server.js --mode development",
    "data:client:hot": "webpack --watch --config webpack.config.js --mode development",
    "data:server:hot:watch": "nodemon --exec babel-node src/server.js --mode development --watch src",
    "data:client:hot:watch": "webpack --watch --config webpack.config.js --mode development --watch src",
    "data:server:hot:watch:debug": "nodemon --exec babel-node src/server.js --mode development --watch src --debug",
    "data:client:hot:watch:debug": "webpack --watch --config webpack.config.js --mode development --watch src --debug",
    "data:server:hot:watch:debug:verbose": "nodemon --exec babel-node src/server.js --mode development --watch src --debug --verbose",
    "data:client:hot:watch:debug:verbose": "webpack --watch --config webpack.config.js --mode development --watch src --debug --verbose",
    "data:server:hot:watch:debug:verbose:short": "nodemon --exec babel-node src/server.js --mode development --watch src --debug --verbose --short",
    "data:client:hot:watch:debug:verbose:short": "webpack --watch --config webpack.config.js --mode development --watch src --debug --verbose --short",
    "data:server:hot:watch:debug:verbose:short:no-color": "nodemon --exec babel-node src/server.js --mode development --watch src --debug --verbose --no-color",
    "data:client:hot:watch:debug:verbose:short:no-color": "webpack --watch --config webpack.config.js --mode development --watch src --debug --verbose --no-color"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^9.1.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "nodemon": "^3.1.7",
    "webpack": "^5.97.1",
    "webpack-cli": "^5.1.4"
  }
}
